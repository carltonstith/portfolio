{
  "routes" : [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "globalHeaders": {
    "Cache-Control": "no-cache"
  },
  "navigationFallback":
    {
      "rewrite": "index.html",
      "exclude": [
        "/images/*.{png,jpg,gif,ico}",
        "/*.{css,scss,js}"
      ]
    }

}
